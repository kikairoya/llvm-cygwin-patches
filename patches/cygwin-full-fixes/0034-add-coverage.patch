 // MSC: $"?to_be_instantiated@?$E@H@@UEAAXXZ" = comdat any
 // MSC: $"?to_be_exported_explicitly@?$E@H@@UEAAXXZ" = comdat any
@@ -86,6 +93,7 @@ template <class T> void E<T>::to_be_exported_explicitly() noexcept {}
