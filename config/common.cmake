set(STAGE1_BINDIR "$ENV{STAGE1_BINDIR}")
if (NOT IS_DIRECTORY "${STAGE1_BINDIR}")
  set(STAGE1_BINDIR "/usr/bin")
endif()
set(CMAKE_ASM_COMPILER "${STAGE1_BINDIR}/clang" CACHE STRING "" FORCE)
set(CMAKE_BUILD_TYPE "Release" CACHE STRING "" FORCE)
set(CMAKE_C_COMPILER "${STAGE1_BINDIR}/clang" CACHE STRING "" FORCE)
set(CMAKE_CXX_COMPILER "${STAGE1_BINDIR}/clang++" CACHE STRING "" FORCE)
set(CMAKE_INSTALL_MESSAGE "NEVER" CACHE STRING "" FORCE)
set(CROSS_TOOLCHAIN_FLAGS_NATIVE "-DCMAKE_BUILD_TYPE=Release;-DCMAKE_ASM_COMPILER=${CMAKE_ASM_COMPILER};-DCMAKE_C_COMPILER=${CMAKE_C_COMPILER};-DCMAKE_CXX_COMPILER=${CMAKE_CXX_COMPILER};-DLLVM_ENABLE_LLD=ON" CACHE STRING "" FORCE)
set(LLVM_FORCE_VC_REVISION "$ENV{BUILD_NAME}" CACHE STRING "" FORCE)
set(LLVM_FORCE_VC_REPOSITORY "llvm-cygwin" CACHE STRING "" FORCE)
